<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TapTrapAttackShowcase"
        tools:targetApi="31">
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".apptouchjacking.AppTouchjackingActivity"
            android:exported="false" />
        <activity
            android:name=".clickjacking.ClickjackingActivity"
            android:exported="false" />
        <activity
            android:name=".controluielements.ControlUiElementsActivity"
            android:exported="false" />
        <activity
            android:name=".deviceadmin.DeviceAdminActivity"
            android:exported="false" />
        <activity
            android:name=".displayoverapps.DisplayOverAppsActivity"
            android:exported="false" />
        <activity
            android:name=".notificationservice.NotificationServiceActivity"
            android:exported="false" />
        <activity
            android:name=".runtimepermissions.RuntimePermissionsActivity"
            android:exported="false" />
        <activity
            android:name=".web.WebActivity"
            android:exported="false" />
    </application>

</manifest>